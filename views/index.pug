extends layout.pug

block content
    .card
        .card-body
            form(
                method="POST"
                action="/data/upload"
                enctype="multipart/form-data"
            )
                .form-group
                    label(for="file") Выберите файл
                    input.form-control-file(type="file" id="file" name="file")
                input.token(type="hidden" name="token")
                button.btn.btn-success Загрузить